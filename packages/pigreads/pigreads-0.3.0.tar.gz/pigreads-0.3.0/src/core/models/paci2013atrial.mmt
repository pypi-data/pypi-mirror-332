[i_f]
E_f = -0.017 [V]
    in [V]
g_f = 30.10312 [S/F]
    in [S/F]
i_f = g_f * (i_f_Xf_gate.Xf * (Membrane.Vm - E_f))
    in [A/F]

[i_f_Xf_gate]
dot(Xf) = (Xf_infinity - Xf) / tau_Xf
    in [1]
Xf_infinity = 1 / (1 + exp((Membrane.Vm * 1000 [1 (0.001)] + 77.85 [mV]) / 5 [mV]))
    in [1]
