alpha_Xs = 1100 / sqrt(1 + exp((-10 [mV] - Membrane.Vm * 1000 [1 (0.001)]) / 6 [mV]))
    in [1]
beta_Xs = 1 / (1 + exp((-60 [mV] + Membrane.Vm * 1000 [1 (0.001)]) / 20 [mV]))
    in [1]
tau_Xs = 1 [s] * (alpha_Xs * beta_Xs) / 1000
