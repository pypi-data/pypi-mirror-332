# Development Environment Configuration
# Copy this to .env for local development

# Application Settings
OPENAGENTS_APP__DEBUG=true
OPENAGENTS_APP__LOG_LEVEL=DEBUG

# Agent Settings (Add your API keys for testing)
OPENAGENTS_AGENT__OPENAI_API_KEY=sk-your-test-key-here
#OPENAGENTS_AGENT__ANTHROPIC_API_KEY=sk-your-test-key-here

# Storage Settings (using SQLite for development)
OPENAGENTS_STORAGE__STORAGE_TYPE=sqlite
OPENAGENTS_STORAGE__DATABASE__PATH=./dev_database.db

# Job Settings
OPENAGENTS_JOB__MAX_CONCURRENT_JOBS=5
